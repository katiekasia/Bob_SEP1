<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<!-- This FXML file defines the layout for editing the  Residential project selected.
     Error labels are included to display validation messages.
     Each child HBox contains labels and text fields for specific project details
     such as title, ID, budget, size, timeline, address, number of floors, and building usage.

     Action buttons like save and cancel are also defined here.
     Error labels are placed to display validation error messages for input fields.
-->
<AnchorPane prefHeight="546.0" prefWidth="564.0" userData="createResidential" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.EditResidential1Controller">
    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="34.0" text="Residential project" textAlignment="CENTER">
            <font>
                <Font size="18.0" />
            </font>
            <padding>
                <Insets left="190.0" />
            </padding>
        </Label>
        <HBox alignment="CENTER_LEFT" layoutX="27.0" layoutY="38.0" prefHeight="30.0" prefWidth="500.0">
            <children>
                <Label prefHeight="20.0" prefWidth="241.0" text="Title">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>

                <TextField fx:id="titleTextField" alignment="CENTER" prefHeight="25.0" prefWidth="150.0" />
                <Label fx:id="errorLabelTitle" prefHeight="20.0" prefWidth="132.0" text="" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>

            </children></HBox>
        <HBox alignment="CENTER_LEFT" layoutX="27.0" layoutY="79.0" prefHeight="30.0" prefWidth="500.0">
            <children>
                <Label prefHeight="20.0" prefWidth="241.0" text="ID ( 6 digits) ">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>


                <TextField fx:id="idTextField" alignment="CENTER" editable="false" prefHeight="25.0" prefWidth="150.0" />
                <Label fx:id="errorLabelId" prefHeight="20.0" prefWidth="138.0" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>

            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="24.0" layoutY="115.0" prefHeight="30.0" prefWidth="500.0">
            <children>
                <Label prefHeight="20.0" prefWidth="226.0" text="Budget">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="budgetTextField" alignment="CENTER" prefHeight="25.0" prefWidth="150.0">
                    <HBox.margin>
                        <Insets left="20.0" />
                    </HBox.margin>
                </TextField>
                <Label fx:id="errorLabelBudget" prefHeight="20.0" prefWidth="138.0" text="" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="25.0" layoutY="156.0" prefHeight="29.0" prefWidth="500.0">
            <children>
                <Label prefHeight="20.0" prefWidth="237.0" text="Size">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="sizeTextField" alignment="CENTER" prefHeight="25.0" prefWidth="150.0">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin></TextField>
                <Label fx:id="errorLabelSize" prefHeight="20.0" prefWidth="138.0" text="" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="26.0" layoutY="197.0" prefHeight="25.0" prefWidth="500.0">
            <children>
                <Label prefHeight="20.0" prefWidth="247.0" text="Timeline">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="timelineTextField" alignment="CENTER" prefHeight="25.0" prefWidth="150.0" />
                <Label fx:id="errorLabelTimeline" prefHeight="20.0" prefWidth="138.0" text="" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="26.0" layoutY="237.0" prefHeight="29.0" prefWidth="500.0">
            <children>
                <Label prefHeight="20.0" prefWidth="231.0" text="Address">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="addressTextField" alignment="CENTER" prefHeight="25.0" prefWidth="150.0">
                    <HBox.margin>
                        <Insets left="15.0" />
                    </HBox.margin></TextField>
                <Label fx:id="errorLabelAddress" prefHeight="20.0" prefWidth="138.0" text="" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="22.0" layoutY="275.0" prefHeight="25.0" prefWidth="500.0">
            <children>
                <Label prefHeight="20.0" prefWidth="252.0" text="Nr. of Kitchens">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="numberOfKitchensTextField" alignment="CENTER" prefHeight="26.0" prefWidth="150.0" />
                <Label fx:id="errorLabelNrOfKitchens" prefHeight="20.0" prefWidth="138.0" text="" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="26.0" layoutY="316.0" prefHeight="38.0" prefWidth="500.0">
            <children>
                <Label prefHeight="20.0" prefWidth="243.0" text="Nr. of bathrooms">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="numberOfBathroomsTextField" alignment="CENTER" prefHeight="25.0" prefWidth="150.0" />
                <Label fx:id="errorLabelNrOfBathrooms" prefHeight="20.0" prefWidth="138.0" text="" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="24.0" layoutY="354.0" prefHeight="25.0" prefWidth="500.0">
            <children>
                <Label prefHeight="20.0" prefWidth="246.0" text=" Nr. of other rooms">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="numberOfOtherRoomsTextField" alignment="CENTER" prefHeight="25.0" prefWidth="150.0" />
                <Label fx:id="errorLabelNrOfRooms" prefHeight="20.0" prefWidth="138.0" text="" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="26.0" layoutY="391.0" prefHeight="26.0" prefWidth="500.0">
            <children>
                <Label prefHeight="20.0" prefWidth="242.0" text="New Building or renovation">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="isNewBuildingTextField" alignment="CENTER" prefHeight="25.0" prefWidth="150.0" />
                <Label prefHeight="20.0" prefWidth="138.0" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER" layoutX="91.0" layoutY="453.0" prefHeight="58.0" prefWidth="382.0" spacing="30.0">
            <children>
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonClicked" prefHeight="36.0" prefWidth="75.0" text="cancel" />
                <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveButtonClicked" prefHeight="35.0" prefWidth="75.0" text="save" />
            </children>
        </HBox>
        <Label fx:id="errorLabelGeneralError" layoutX="182.0" layoutY="443.0" prefHeight="20.0" prefWidth="90.0" textFill="#ff1100">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
